<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemo.WelcomeController">
    <children>
        <VBox alignment="CENTER" prefHeight="373.0" prefWidth="530.0" spacing="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome!">
               <font>
                  <Font size="43.0" />
               </font>
            </Text>
                <HBox alignment="TOP_CENTER" prefHeight="55.0" prefWidth="518.0" spacing="10.0">
                    <children>
                        <Button fx:id="register" mnemonicParsing="false" onAction="#handleregister" prefHeight="30.0" prefWidth="80.0" text="Register" />
                        <Button fx:id="login" onAction="#handlesignin" prefHeight="30.0" prefWidth="90.0" text="Login" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
